VITE_APP_API_URL=https://api.voiceagent.com/api/v1
VITE_APP_WEBSOCKET_URL=wss://api.voiceagent.com/ws/v1
VITE_APP_ENVIRONMENT=development

# Authentication Settings
VITE_APP_AUTH_ENABLED=true

# Voice Processing Parameters
# Voice activity detection threshold in dB
VITE_APP_VOICE_ACTIVITY_THRESHOLD=-26
# Noise floor level in dB for audio processing
VITE_APP_NOISE_FLOOR=-45
# Audio sampling rate in Hz
VITE_APP_SAMPLE_RATE=16000

# WebSocket Configuration
# Heartbeat interval in milliseconds
VITE_APP_WEBSOCKET_HEARTBEAT_INTERVAL=30000
# Maximum WebSocket reconnection attempts
VITE_APP_WEBSOCKET_RECONNECT_ATTEMPTS=5